    "example-lifecycle": "pulp build --include examples/lifecycle/src --to examples/lifecycle/dist/example.js",
    "example-todo": "pulp build --include examples/todo/src --to examples/todo/dist/example.js",
    "example-i18n": "pulp build --include examples/i18n/src --to examples/i18n/dist/example.js"
  },
  "devDependencies": {
    "pulp": "^11.0.0",
    "purescript": "^0.11.0",
    "purescript-psa": "^0.5.0",
    "rimraf": "^2.5.4"
  }
