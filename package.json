    "example-basic": "pulp build --include examples/basic/src --to examples/basic/dist/example.js",
    "example-components": "pulp build --include examples/components/src --to examples/components/dist/example.js",
    "example-components-inputs": "pulp build --include examples/components-inputs/src --to examples/components-inputs/dist/example.js",
    "example-components-multitype": "pulp build --include examples/components-multitype/src --to examples/components-multitype/dist/example.js",
    "example-driver-io": "pulp build --include examples/driver-io/src --to examples/driver-io/dist/example.js",
    "example-driver-routing": "pulp build --include examples/driver-routing/src --to examples/driver-routing/dist/example.js",
    "example-driver-websockets": "pulp build --include examples/driver-websockets/src --to examples/driver-websockets/dist/example.js",
    "example-effects-aff-ajax": "pulp build --include examples/effects-aff-ajax/src --to examples/effects-aff-ajax/dist/example.js",
    "example-effects-eff-random": "pulp build --include examples/effects-eff-random/src --to examples/effects-eff-random/dist/example.js",
    "example-higher-order-components": "pulp build --include examples/higher-order-components/src --to examples/higher-order-components/dist/example.js",
